<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="springHelloWorld" class="com.javassh.tutorial.ioc.helloworld.impl.SpringHelloWorld"></bean>
	<bean id="structsHelloWorld" class="com.javassh.tutorial.ioc.helloworld.impl.StructsHelloWorld"></bean>
	<bean id="helloWorldService" class="com.javassh.tutorial.ioc.helloworld.HelloWorldService">
		<property name="hwHandler" ref="springHelloWorld"/>
	</bean>
</beans>